         <font>
            <Font name="Poppins Regular" size="11.0" />
         </font>
      </Label>
      <DatePicker fx:id="Est" layoutX="248.0" layoutY="303.0" prefHeight="28.0" prefWidth="124.0" />
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#990000" height="508.0" layoutY="-1.0" stroke="TRANSPARENT" strokeType="INSIDE" width="200.0">
         <rotationAxis>
            <Point3D />
         </rotationAxis>
      </Rectangle>
      <Button fx:id="backButton" layoutX="24.0" layoutY="25.0" mnemonicParsing="false" onAction="#handleBackButtonAction" text="Back" />
