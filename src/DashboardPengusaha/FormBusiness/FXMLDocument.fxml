         <font>
            <Font name="Poppins Regular" size="11.0" />
         </font>
      </Label>
      <TextField fx:id="Location" layoutX="248.0" layoutY="242.0" prefHeight="28.0" prefWidth="298.0" promptText="enter your business location" />
      <Label fx:id="labelLoc" layoutX="250.0" layoutY="222.0" minHeight="16" minWidth="69" text="Location">
         <font>
            <Font name="Poppins Regular" size="11.0" />
         </font>
      </Label>
      <Label fx:id="labelEst" layoutX="250.0" layoutY="280.0" minHeight="16" minWidth="69" text="Date of establishment">
         <font>
            <Font name="Poppins Regular" size="11.0" />
         </font>
      </Label>
      <ChoiceBox fx:id="category" layoutX="422.0" layoutY="303.0" opacity="0.85" prefHeight="28.0" prefWidth="123.0" />
